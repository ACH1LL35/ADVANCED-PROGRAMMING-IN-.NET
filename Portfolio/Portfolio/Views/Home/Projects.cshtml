@{
    ViewBag.Title = "Projects";
}
<main aria-labelledby="title">
    <h3>Projects</h3>
    <table class="table table-bordered">
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Course</th>
        </tr>
        @foreach (var item in ViewBag.Projects)
        {
            <tr>
                <td><a href="/home/projects">@item.Title</a></td>
                <td>@item.Des</td>
                <td>@item.Course</td>
            </tr>
        }
    </table>
</main>